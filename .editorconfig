root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.yml]
indent_size = 2
indent_style = space

[package*.json]
indent_size = 2
indent_style = space

[dep5]
indent_size = 2
indent_style = space
